{
  "name": "fetzi/state",
  "description": "Laravel package for storing current application state in cache/session",
  "homepage": "https://github.com/fetzi/state",
  "authors": [
    {
      "name": "Johannes Pichler",
      "email": "johannes.pichler@fetzi.io"
    }
  ],
  "keywords": [
    "state",
    "stack",
    "session",
    "cache",
    "cacheable",
    "laravel",
    "login"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "illuminate/support": "^5.2",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^1.12",
    "squizlabs/php_codesniffer": "^2.7",
    "phpspec/phpspec": "^3.2",
    "henrikbjorn/phpspec-code-coverage": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Fetzi\\State\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "php-cs": "vendor/bin/phpcs src/ --standard=PSR2",
    "php-cs-fix": "vendor/bin/php-cs-fixer fix src/",
    "test": "vendor/bin/phpspec run"
  }
}
